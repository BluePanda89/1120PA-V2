{
  "extends": "astro/tsconfigs/base",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist",".vscode",".gitignore",".prettierrc"],
  "compilerOptions": {
    "plugins": [{"name": "@astrojs/ts-plugin"}],
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@styles/*": ["./src/styles/*"],
      "@data/*": ["./src/data/*"]
      }
}
}